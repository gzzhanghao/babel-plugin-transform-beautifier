{
  "name": "babel-plugin-transform-beautifier",
  "version": "0.1.0",
  "description": "A babel plugin that beautifies javascripts",
  "main": "dist/index.js",
  "module": "dist/esm/index.mjs",
  "author": "Jason <jason.daurus@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:cjs": "swc src -o dist/index.js",
    "build:mjs": "swc src -o dist/esm/index.mjs -C module.type=es6",
    "dev": "jest --watch --coverage",
    "test": "jest"
  },
  "homepage": "https://github.com/gzzhanghao/babel-plugin-transform-beautifier#readme",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@babel/types": "^7.21.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.37",
    "@types/babel__core": "^7.20.0",
    "@types/deindent": "^0.1.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^5.54.0",
    "concurrently": "^7.6.0",
    "deindent": "^0.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^26.6.3",
    "typescript": "^4.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gzzhanghao/babel-plugin-transform-beautifier.git"
  },
  "bugs": {
    "url": "https://github.com/gzzhanghao/babel-plugin-transform-beautifier/issues"
  }
}
