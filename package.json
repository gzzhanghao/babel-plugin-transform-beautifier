{
  "name": "babel-plugin-transform-beautifier",
  "version": "0.1.0",
  "description": "A babel plugin that beautifies javascripts",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "author": "Jason <jason.daurus@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:cjs": "swc src -o dist/index.cjs",
    "build:mjs": "swc src -o dist/index.js -C module.type=es6",
    "dev": "vitest",
    "test": "vitest run"
  },
  "homepage": "https://github.com/gzzhanghao/babel-plugin-transform-beautifier#readme",
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/types": "^7.26.9",
    "@eslint/js": "^10.0.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.16",
    "@types/babel__core": "^7.20.5",
    "@types/deindent": "^0.1.3",
    "@types/jest": "^29.5.14",
    "concurrently": "^9.1.2",
    "deindent": "^0.1.0",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vitest": "^3.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gzzhanghao/babel-plugin-transform-beautifier.git"
  },
  "bugs": {
    "url": "https://github.com/gzzhanghao/babel-plugin-transform-beautifier/issues"
  }
}
